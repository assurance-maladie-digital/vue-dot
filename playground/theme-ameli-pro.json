{
	"name": "ameli-pro",
	"version": "0.0.1",
	"config": {
		"styles": {
			".v-alert": {
				"top": "0",
				"width": "100%",
				"position": "absolute"
			},
			".v-input--switch__thumb": {
				"box-shadow": "none !important"
			},
			".v-input--switch.v-input--is-label-active .v-input--switch__track": {
				"color": "#d1eaff !important"
			},
			".v-input--switch.v-input--is-label-active .v-input--switch__thumb": {
				"color": "#0092a8 !important"
			},
			".v-input--switch:not(.v-input--is-label-active) .v-input--switch__track": {
				"color": "#ccc !important"
			},
			".v-input--switch:not(.v-input--is-label-active) .v-input--switch__thumb": {
				"color": "#808080 !important"
			},
			".v-toolbar": {
				"box-shadow": "none"
			},
			".v-text-field": {
				"padding-top": "0"
			},
			".v-text-field .v-input__slot": {
				"padding": "0 5px",
				"background": "#fff",
				"border-radius": "5px",
				"border": "1px solid #bdbdbd"
			},
			".v-text-field .v-input__slot::before, .v-text-field .v-input__slot::after": {
				"content": "none !important"
			},
			".v-menu__content": {
				"box-shadow": "none",
				"border": "1px solid #bdbdbd"
			},
			".v-navigation-drawer *": {
				"color": "#fff !important"
			},
			".v-toolbar .v-btn.theme--dark[primary]": {
				"color": "#084870"
			},
			".v-toolbar .v-btn:not(.theme--dark)[primary]": {
				"color": "#fff !important"
			},
			".theme--dark p": {
				"color": "#fff"
			},
			".theme--dark.v-text-field .v-input__slot": {
				"background": "rgba(255, 255, 255, .19)"
			},
			".theme--dark h1, .theme--dark h2, .theme--dark h3, .theme--dark h4, .theme--dark h5": {
				"color": "#fff"
			}
		},
		"typography": {
			"styles": {
				".application" : {
					"font-family": "'Open Sans', sans-serif",
					"line-height": "1.5"
				},
				"h1": {
					"font-family": "OSP-DIN, sans-serif",
					"font-size": "2.5rem",
					"font-weight": "400",
					"text-transform": "uppercase",
					"color": "#01b9f5"
				},
				"h2": {
					"font-family": "'Open Sans', sans-serif",
					"font-size": "1.13rem",
					"color": "#333",
					"font-weight": "600"
				},
				"h3": {
					"font-family": "'Open Sans', sans-serif",
					"font-size": "0.94rem",
					"color": "#333",
					"font-weight": "600"
				},
				"h4": {
					"font-family": "'Open Sans', sans-serif",
					"font-size": "0.88rem",
					"color": "#333",
					"font-weight": "600"
				},
				"h5": {
					"font-family": "'Open Sans', sans-serif",
					"font-size": "0.81rem",
					"color": "#333",
					"font-weight": "600"
				},
				"a": {
					"font-family": "'Open Sans', sans-serif",
					"font-size": "0.81rem",
					"color": "#005da8",
					"font-weight": "400",
					"text-decoration": "none"
				},
				"p": {
					"font-family": "'Open Sans', sans-serif",
					"font-size": "0.81rem",
					"color": "#333",
					"font-weight": "400"
				},
				".font-weight-medium": {
					"font-weight": "600 !important"
				},
				".nota": {
					"font-size": "0.69rem"
				}
			}
		},
		"colors": {
			"primary": "#084870",
			"secondary": "#424242",
			"accent": "#01b9f5",
			"error": "#941900",
			"info": "#1cb0ba",
			"success": "#4caf50",
			"warning": "#b35a00",
			"shades": {
				"base": "#808080",
				"darken1": "#333",
				"black": "#000",
				"lighten1": "#ccc",
				"lighten2": "#e0e0e0",
				"lighten3": "#ededed",
				"lighten4": "#f5f5f5",
				"lighten5": "#fafafa",
				"white": "#fff"
			},
			"blue": {
				"base": "#0087cf",
				"darken1": "#005da8",
				"darken2": "#0c4887",
				"darken3": "#084870",
				"darken4": "#003c61",
				"lighten1": "#01b9f5",
				"lighten2": "#9edfff",
				"lighten3": "#bdf3dd",
				"lighten4": "#ebf8ff"
			},
			"blue-green": {
				"base": "#1cb0ba",
				"darken1": "#0092a8",
				"darken2": "#006a77",
				"lighten1": "#58d4db",
				"lighten2": "#bff9ff",
				"lighten3": "#e3faff"
			},
			"green": {
				"base": "#c3cc1e",
				"darken1": "#a4c242",
				"darken2": "#839e29",
				"lighten1": "#e4ed2f",
				"lighten2": "#f2f799"
			},
			"yellow": {
				"base": "#fac746",
				"darken1": "#f7b512",
				"darken2": "#de9f00",
				"lighten1": "#fed66d",
				"lighten2": "#ffe7ab"
			},
			"brown": {
				"base": "#ef8402",
				"darken1": "#d46e00",
				"darken2": "#b35a00",
				"lighten1": "#ffc57d",
				"lighten2": "#ffd8b0"
			},
			"red": {
				"base": "#f26864",
				"darken1": "#c75329",
				"darken2": "#941900",
				"lighten1": "#fc9d94",
				"lighten2": "#ffcec7"
			}
		},
		"icons": [
			{
				"name": "svg",
				"svg": "<svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 400 400\"><path fill=\"#4dba87\" d=\"M237.42 86.66L207.19 139l-30.22-52.35H76.3l130.9 226.69L338.07 86.66z\"/><path fill=\"#435466\" d=\"M237.42 86.66L207.19 139l-30.22-52.35h-48.3l78.52 136 78.53-136z\"/></svg>"
			}
		]
	}
}
